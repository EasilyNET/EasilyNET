<Project Sdk="Microsoft.NET.Sdk">
	<Import Project="..\..\build\common.props" />

	<PropertyGroup>
		<AssemblyTitle>$(PackagePrefix).Images</AssemblyTitle>
		<AssemblyName>$(PackagePrefix).Images</AssemblyName>
		<PackageId>$(PackagePrefix).Images</PackageId>
		<PackageTags>QrCode</PackageTags>
		<PackageReleaseNotes>
		</PackageReleaseNotes>
		<RootNamespace>EasilyNET.Images</RootNamespace>
	</PropertyGroup>
	
	<PropertyGroup>
        <RepositoryUrl>https://github.com/EasilyNET/EasilyNET.Images</RepositoryUrl>
        <Description>含QRCode创建和解析工具,无需自行使用apt等工具安装组件</Description>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="SkiaSharp.NativeAssets.Linux.NoDependencies" Version="2.88.6" />
        <PackageReference Include="ZXing.Net" Version="0.16.9" />
    </ItemGroup>

</Project>
